{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "baseUrl": "./",
    "incremental": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "pretty": true,
    "lib": ["esnext"]
  },
  "exclude": ["node_modules", "dist"]
}
